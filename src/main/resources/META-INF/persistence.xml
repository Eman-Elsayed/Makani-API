<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="makanypu" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>com.mycompany.makanyapi.models.UserEvent</class>
    <class>com.mycompany.makanyapi.models.Event</class>
    <class>com.mycompany.makanyapi.models.Freelancer</class>
    <class>com.mycompany.makanyapi.models.Room</class>
    <class>com.mycompany.makanyapi.models.Speaker</class>
    <class>com.mycompany.makanyapi.models.Facility</class>
    <class>com.mycompany.makanyapi.models.SpacePicture</class>
    <class>com.mycompany.makanyapi.models.SpaceContact</class>
    <class>com.mycompany.makanyapi.models.User</class>
    <class>com.mycompany.makanyapi.models.Space</class>
    <class>com.mycompany.makanyapi.models.SpaceLocation</class>
    <class>com.mycompany.makanyapi.models.Interest</class>
    <class>com.mycompany.makanyapi.models.Skill</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/makani?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
    </properties>
  </persistence-unit>
</persistence>
